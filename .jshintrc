{
	"bitwise"  : true,
	"camelcase": true,
	"eqeqeq"   : true,
	"esnext"   : true,
	"freeze"   : true,
	"globals"  : {
		"$"                 : true,
		"module"            : true,
		"process"           : true,
		"regeneratorRuntime": true,
		"require"           : true,
		"window"            : true,
		"alert"            : true,
		"document"			: true
	},
	"immed"   : true,
	"latedef" : false,
	"maxerr"  : 10,
	"newcap"  : false,
	"noarg"   : true,
	"node"    : true,
	"noempty" : true,
	"nonbsp"  : true,
	"nonew"   : true,
	"strict"  : true,
	"undef"   : true,
	"unused"  : true
}
